{
    "name":"hierarchicalmgp-fold1-gpu",
    "algorithm":"BayesianOptimization",
    "trialNumber":10,
    "concurrency":26,
    "path":"~/mgp-rnn-mimic/",
    "command":"python ./hierarchical-mgp-rnn.py data 1",
    "search_space": {
        "goal":"MAXIMIZE",
        "randomInitTrials":1,
        "maxTrials":35,
        "maxParallelTrials":10,
        "params":[
            {
                "parameterName":"lr",
                "type":"DOUBLE",
                "minValue":0.00001,
                "maxValue":0.001,
                "feasiblePoints":"",
                "scalingType":"LINEAR"
            },
            {
                "parameterName":"l2_penalty",
                "type":"DOUBLE",
                "minValue":1e-5,
                "maxValue":1e-2,
                "scalingType":"LINEAR"
            },
            {
                "parameterName":"epochs",
                "type":"INTEGER",
                "minValue":40,
                "maxValue":90,
                "scalingType":"LINEAR"
            },
            {
                "parameterName":"n_layers",
                "type":"DISCRETE",
                "minValue":"",
                "maxValue":"",
                "feasiblePoints":"2,3,4",
                "scalingType":"LINEAR"
            }
        ]
    }
}
