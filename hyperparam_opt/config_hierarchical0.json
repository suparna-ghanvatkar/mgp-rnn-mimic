{
    "name":"mts_rnn",
    "algorithm":"BayesianOptimization",
    "trialNumber":40,
    "concurrency":40,
    "path":"~/mgp-rnn-mimic/hyperparam_opt",
    "command":"python ./hierarchical-mgp-rnn.py data trainval 0",
    "search_space": {
        "goal":"MAXIMIZE",
        "randomInitTrials":40,
        "maxTrials":40,
        "maxParallelTrials":40,
        "params":[
            {
                "parameterName":"lr",
                "type":"DOUBLE",
                "minValue":0.00001,
                "maxValue":0.01,
                "feasiblePoints":"",
                "scalingType":"LINEAR"
            },
            {
                "parameterName":"l2_penalty",
                "type":"DOUBLE",
                "minValue":1e-5,
                "maxValue":1e-2,
                "scalingType":"LINEAR"
            },
            {
                "parameterName":"beta1",
                "type":"DOUBLE",
                "minValue":0.01,
                "maxValue":0.99,
                "feasiblePoints":"",
                "scalingType":"LINEAR"
            },
            {
                "parameterName":"beta2",
                "type":"DOUBLE",
                "minValue":0.1,
                "maxValue":0.9999,
                "feasiblePoints":"",
                "scalingType":"LINEAR"
            },
            {
                "parameterName":"epsilon",
                "type":"DOUBLE",
                "minValue":1e-8,
                "maxValue":1,
                "feasiblePoints":"",
                "scalingType":"LINEAR"
            }
        ]
    }
}
