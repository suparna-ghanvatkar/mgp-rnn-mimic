{
    "name":"hierarchicalmgp-fold0-train",
    "algorithm":"BayesianOptimization",
    "trialNumber":10,
    "concurrency":26,
    "path":"~/mgp-rnn-mimic/",
    "command":"python ./hierarchical-mgp-rnn.py data 0",
    "search_space": {
        "goal":"MAXIMIZE",
        "randomInitTrials":1,
        "maxTrials":35,
        "maxParallelTrials":10,
        "params":[
            {
                "parameterName":"lr",
                "type":"DOUBLE",
                "minValue":0.00001,
                "maxValue":0.001,
                "feasiblePoints":"",
                "scalingType":"LINEAR"
            },
            {
                "parameterName":"l2_penalty",
                "type":"DOUBLE",
                "minValue":1e-5,
                "maxValue":1e-2,
                "scalingType":"LINEAR"
            },
            {
                "parameterName":"epochs",
                "type":"INTEGER",
                "minValue":60,
                "maxValue":150,
                "scalingType":"LINEAR"
            },
            {
                "parameterName":"beta1",
                "type":"DOUBLE",
                "minValue":0.01,
                "maxValue":0.99,
                "feasiblePoints":"",
                "scalingType":"LINEAR"
            },
            {
                "parameterName":"beta2",
                "type":"DOUBLE",
                "minValue":0.1,
                "maxValue":0.9999,
                "feasiblePoints":"",
                "scalingType":"LINEAR"
            },
            {
                "parameterName":"epsilon",
                "type":"DOUBLE",
                "minValue":1e-8,
                "maxValue":1,
                "feasiblePoints":"",
                "scalingType":"LINEAR"
            }
        ]
    }
}
